{
  "name": "@oplayer/ui",
  "version": "1.0.7",
  "description": "Snow theme for oplayer.",
  "type": "module",
  "main": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "author": "",
  "homepage": "https://github.com/shiyiya/oplayer",
  "keywords": [
    "oplayer-theme"
  ],
  "license": "MIT",
  "scripts": {
    "start": "concurrently -n es,dts \"vite build --watch\"  \"tsc --emitDeclarationOnly --watch\"",
    "tsc": "tsc --noEmit && echo",
    "build": "vite build &&  tsc --emitDeclarationOnly",
    "babel": "../../node_modules/.bin/babel dist/index.es.js --config-file ../../babel.config.js -format --out-file dist/index.min.js --minified  --module-id OUI"
  },
  "files": [
    "dist",
    "src"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not op_mini all",
    "IE 11"
  ],
  "peerDependencies": {
    "@oplayer/core": ">=1.0.0"
  },
  "devDependencies": {
    "@oplayer/core": "workspace:*"
  }
}
